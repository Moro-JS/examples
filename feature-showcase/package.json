{
  "name": "feature-showcase-example",
  "version": "1.0.0",
  "private": true,
  "description": "Comprehensive feature showcase demonstrating advanced Moro framework capabilities",
  "main": "src/server.ts",
  "scripts": {
    "dev": "tsx src/server.ts",
    "dev:pure": "tsx src/server.ts",
    "dev:framework": "tsx watch src/server.ts",
    "dev:validation": "tsx src/validation-examples.ts",
    "dev:routing": "tsx src/intelligent-routing-demo.ts",
    "dev:docs": "tsx src/api-docs-demo.ts",
    "dev:config": "tsx src/config-demo.ts",
    "dev:di": "tsx src/functional-di-demo.ts",
    "dev:headers": "tsx src/response-headers-test.ts",
    "dev:logging": "tsx -e \"import('./src/advanced-logging-demo').then(m => m.createLoggingDemoServer().listen(3001, () => console.log('Logging demo server running on http://localhost:3001')))\"",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "echo \"No tests specified\"",
    "switch:local": "rm -rf node_modules package-lock.json && npm install file:../../MoroJS",
    "switch:npm": "rm -rf node_modules package-lock.json && npm install @morojs/moro@latest"
  },
  "dependencies": {
    "@morojs/moro": "latest",
    "reflect-metadata": "^0.2.2",
    "swagger-ui-dist": "^5.29.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "moro",
    "api",
    "typescript",
    "demo"
  ],
  "author": "MoroJS Team"
}
