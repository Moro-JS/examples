{
  "name": "simple-api-example",
  "version": "1.0.0",
  "private": true,
  "description": "Simple API demonstration showing basic Moro framework usage",
  "main": "src/server.ts",
  "scripts": {
    "dev": "NODE_ENV=development ts-node src/server.ts",
    "dev:watch": "NODE_ENV=development ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "start": "NODE_ENV=production node dist/server.js",
    "test": "echo \"No tests specified\"",
    "switch:local": "rm -rf node_modules package-lock.json && npm install file:../../MoroJS",
    "switch:npm": "rm -rf node_modules package-lock.json && npm install @morojs/moro@latest"
  },
  "dependencies": {
    "@morojs/moro": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.2"
  },
  "keywords": [
    "moro",
    "api",
    "simple",
    "example"
  ]
}
