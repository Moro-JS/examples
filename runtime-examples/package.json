{
  "name": "morojs-runtime-examples",
  "version": "1.0.0",
  "description": "MoroJS Runtime Examples - Multi-environment support",
  "main": "node-example.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only node-example.ts",
    "dev:node": "ts-node-dev --respawn --transpile-only node-example.ts",
    "build": "tsc",
    "start": "node dist/node-example.js",
    "test:runtime": "ts-node --transpile-only test-runtime-system.ts",
    "switch:local": "rm -rf node_modules package-lock.json && npm install file:../../MoroJS",
    "switch:npm": "rm -rf node_modules package-lock.json && npm install @morojs/moro@latest"
  },
  "keywords": [
    "morojs",
    "runtime",
    "serverless",
    "edge",
    "lambda",
    "vercel",
    "cloudflare"
  ],
  "author": "MoroJS Team",
  "license": "MIT",
  "dependencies": {
    "@morojs/moro": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "ts-node": "^10.9.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.0"
  }
}
