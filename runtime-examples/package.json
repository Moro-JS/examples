{
  "name": "morojs-runtime-examples",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "MoroJS Runtime Examples - Multi-environment support",
  "main": "node-example.js",
  "scripts": {
    "dev": "tsx node-example.ts",
    "dev:watch": "tsx watch node-example.ts",
    "build": "tsc",
    "start": "node dist/node-example.js",
    "test:runtime": "tsx test-runtime-system.ts",
    "switch:local": "rm -rf node_modules package-lock.json && npm install file:../../MoroJS",
    "switch:npm": "rm -rf node_modules package-lock.json && npm install @morojs/moro@latest"
  },
  "keywords": [
    "morojs",
    "runtime",
    "serverless",
    "edge",
    "lambda",
    "vercel",
    "cloudflare"
  ],
  "author": "MoroJS Team",
  "license": "MIT",
  "dependencies": {
    "@morojs/moro": "latest"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsx": "^2.0.0",
    "typescript": "^5.0.0"
  }
}
